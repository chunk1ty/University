@model IList<Ankk.Web.Models.AnswerViewModel>
@{
    ViewBag.Title = "UserResult";
}

<h2>Source code</h2>

<hr />
@*@Html.ActionLink("Back", "View", "Administration", new { Id = @Model[0].Questions.ContestId }, new { @class = "btn btn-primary" })*@

@for (int i = 0; i < Model.Count(); i++)
{
    <h5>Task number: @(i+1)</h5>
    <h5>Points: @Model[i].Points</h5>
    @Html.Raw("<pre>" + Html.Encode(Model[i].SourceCode) + "</pre>")
    <br />
} 
